{
  "coc.preferences.formatOnType": true,
  "coc.preferences.formatOnSaveFiletypes": ["scala", "rust"],
  "coc.source.buffer.disableSyntaxes": ["comment", "string"],
  "codeLens.enable": true,
  "metals.statusBarEnabled": true,
  "diagnostic.checkCurrentLine": true,
  "coc.preferences.useQuickfixForLocations": true,
  "suggest.fixInsertedWord": false,
  "rust-analyzer.completion.addCallParenthesis": false,
  "rust-analyzer.checkOnSave.command": "clippy",
  "diagnostic.displayByAle": false,
}
